###  http://CamaraMadrid.org/hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:range skos:Concept ;
             rdfs:comment "Indicates the location where the camera is installed." ;
             rdfs:label "hasLocation" .


###  http://CamaraMadrid.org/hasResolution
:hasResolution rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:range xsd:string ;
               rdfs:comment "Indicates the resolution of the camera." ;
               rdfs:label "hasResolution" .


###  http://CamaraMadrid.org/hasType
:hasType rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:range xsd:string ;
         rdfs:comment "Indicates the type of the camera." ;
         rdfs:label "hasType" .


###  http://CamaraMadrid.org/videocameras/hasYearOfAcquisition
:hasYearOfAcquisition rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:range xsd:integer ;
                      rdfs:comment "Indicates the year when the camera was acquired." ;
                      rdfs:label "hasYearOfAcquisition" .


###  http://CamaraMadrid.org/videocameras/hasZoom
:hasZoom rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:range xsd:string ;
         rdfs:comment "Indicates the optical zoom capacity of the camera." ;
         rdfs:label "hasZoom" .




