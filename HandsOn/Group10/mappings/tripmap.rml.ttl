@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix bicimap: <https://bicimap.com/ontology#> .
@prefix schema: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


<#BiketripsMapping>
    rml:logicalSource [
        rml:source "biketrips.csv" ;
        rml:referenceFormulation ql:CSV
    ] ;

    rr:subjectMap [
        rr:template "http://bicimap.com/entity/Trip{id_trip}" ;
        rr:class bicimap:BikeTrip
    ] ;

    rr:predicateObjectMap [
        rr:predicate schema:id ;
        rr:objectMap [
            rml:reference "id_trip" ;
            rr:termType rr:Literal;
            rr:datatype xsd:int
        ]
    ] ;

    rr:predicateObjectMap [
        rr:predicateMap [
            rr:constant bicimap:bikeId;
            rr:termType rr:IRI
        ] ;
        rr:objectMap [
            rml:reference "id_Bike" ;
            rr:termType rr:Literal ;
            rr:datatype xsd:integer
        ]
    ] ;

#    rr:predicateObjectMap [
#        rr:predicateMap [
#            rr:constant bicimap:tripMinutes;
#            rr:termType rr:IRI
#        ] ;
#        rr:objectMap [
#            rml:reference "duration" ;
#            rr:termType rr:Literal ;
#            rr:datatype xsd:double
#        ]
#    ] ;

    rr:predicateObjectMap [
        rr:predicateMap [
            rr:constant bicimap:lockDate;
            rr:termType rr:IRI
        ] ;
        rr:objectMap [
            rml:reference "lock_date" ;
            rr:termType rr:Literal ;
            rr:datatype xsd:datetime
        ]
    ] ;

    rr:predicateObjectMap [
        rr:predicateMap [
            rr:constant bicimap:unlockDate;
            rr:termType rr:IRI
        ] ;
        rr:objectMap [
            rml:reference "unlock_date" ;
            rr:termType rr:Literal ;
            rr:datatype xsd:datetime
        ]
    ] ;

    rr:predicateObjectMap [
        rr:predicateMap [
            rr:constant bicimap:unlockDock;
            rr:termType rr:IRI
        ] ;
        rr:objectMap [
            rml:reference "dock_unlock" ;
            rr:termType rr:Literal ;
            rr:datatype xsd:integer
        ]
    ] ;

    rr:predicateObjectMap [
        rr:predicateMap [
            rr:constant bicimap:lockDock;
            rr:termType rr:IRI
        ] ;
        rr:objectMap [
            rml:reference "dock_unlock" ;
            rr:termType rr:Literal ;
            rr:datatype xsd:integer
        ]
    ] ;

    rr:predicateObjectMap [
        rr:predicateMap [
            rr:constant bicimap:startAt;
            rr:termType rr:IRI
        ] ;
        rr:objectMap [
            rr:template "http://bicimap.com/places/{id_place_unlock}"
        ]
    ] ;

    rr:predicateObjectMap [
        rr:predicateMap [
            rr:constant bicimap:endAt;
            rr:termType rr:IRI
        ] ;
        rr:objectMap [
            rr:template "http://bicimap.com/places/{id_place_lock}"
        ]
    ] .

