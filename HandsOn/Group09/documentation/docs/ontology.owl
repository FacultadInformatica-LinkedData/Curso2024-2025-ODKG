    <!-- http://mAIRdrid.org/ontology/AirQualityMetric -->


    <owl:Class rdf:about="http://mAIRdrid.org/ontology/AirQualityMetric">
        <rdfs:comment xml:lang="en">An air quality metric is a measurable property of air quality, such as the concentration of specific pollutants (e.g., CO), observed by sensors at a particular air quality station.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una métrica de calidad del aire es una propiedad medible de la calidad del aire, como la concentración de contaminantes específicos (p. ej. CO), observada por sensores en una estación de calidad del aire determinada.</rdfs:comment>
        <rdfs:label xml:lang="en">Air Quality Metric</rdfs:label>
        <rdfs:label xml:lang="es">Métrica de Calidad del Aire</rdfs:label>
    </owl:Class>
    


    <!-- http://mAIRdrid.org/ontology/Location -->


    <owl:Class rdf:about="http://mAIRdrid.org/ontology/Location">
        <rdfs:comment xml:lang="en">A location is a physical point where a station is placed</rdfs:comment>
        <rdfs:comment xml:lang="es">Una ubicación es la posición geográfica donde se encuentra una estación</rdfs:comment>
        <rdfs:label xml:lang="en">Location</rdfs:label>
        <rdfs:label xml:lang="es">Ubicación</rdfs:label>
    </owl:Class>
    


    <!-- http://mAIRdrid.org/ontology/Station -->


    <owl:Class rdf:about="http://mAIRdrid.org/ontology/Station">
        <rdfs:comment xml:lang="en">A station is a located element that contains sensors</rdfs:comment>
        <rdfs:comment xml:lang="es">Una estación es un elemento localizado que contiene sensores</rdfs:comment>
        <rdfs:label xml:lang="es">Estación</rdfs:label>
        <rdfs:label xml:lang="en">Station</rdfs:label>
    </owl:Class>
    


    <!-- http://mAIRdrid.org/ontology/nationalStationID -->


    <owl:Class rdf:about="http://mAIRdrid.org/ontology/nationalStationID">
        <rdfs:comment xml:lang="en">A unique identifier assigned to an air quality station at the national level.</rdfs:comment>
        <rdfs:comment xml:lang="es">Un identificador único asignado a una estación de calidad del aire a nivel nacional.</rdfs:comment>
        <rdfs:label xml:lang="es">ID de Estación Nacional</rdfs:label>
        <rdfs:label xml:lang="en">National Station ID</rdfs:label>
    </owl:Class>
    


    <!-- http://qudt.org/schema/qudt#QuantityValue -->


    <owl:Class rdf:about="http://qudt.org/schema/qudt#QuantityValue">
        <rdfs:comment xml:lang="en">A numeric value representing the measured quantity of a specific air quality metric.</rdfs:comment>
        <rdfs:comment xml:lang="es">Un valor numérico que representa la cantidad medida de un indicador específico de calidad del aire.</rdfs:comment>
        <rdfs:label xml:lang="en">Quantity Value</rdfs:label>
        <rdfs:label xml:lang="es">Valor de Cantidad</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/sosa/Observation -->


    <owl:Class rdf:about="http://www.w3.org/ns/sosa/Observation">
        <rdfs:comment xml:lang="en">A record of an observed phenomenon, including the features of interest, results, the sensor that made the observation, and the property that was observed.</rdfs:comment>
        <rdfs:comment xml:lang="es">Un registro de un fenómeno observado, incluyendo las características de interés, resultados, el sensor que realizó la observación y la propiedad que fue observada.</rdfs:comment>
        <rdfs:label xml:lang="es">Observación</rdfs:label>
        <rdfs:label xml:lang="en">Observation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/sosa/Sensor -->


    <owl:Class rdf:about="http://www.w3.org/ns/sosa/Sensor">
        <rdfs:comment xml:lang="en">A device that observes a phenomenon, providing data about specific properties of interest.</rdfs:comment>
        <rdfs:comment xml:lang="es">Un dispositivo que observa un fenómeno, proporcionando datos sobre propiedades específicas de interés.</rdfs:comment>
        <rdfs:label xml:lang="en">Sensor</rdfs:label>
        <rdfs:label xml:lang="es">Sensor</rdfs:label>
    </owl:Class>
