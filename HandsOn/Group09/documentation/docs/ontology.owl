    <owl:Ontology rdf:about="https://mAIRdrid.org/ontology">
        <owl:versionIRI rdf:resource="https://mAIRdrid.org/ontology/1.0.0"/>
        <terms:abstract xml:lang="en">This ontology provides a comprehensive framework for representing air quality metrics, including concentrations of pollutants such as NOx, CO, PM2.5, PM10, and BTX, as well as the corresponding measurement values, timestamps, and metadata about monitoring stations and their sensors. The ontology aims to facilitate data interoperability and sharing among environmental monitoring systems, researchers, and policy-makers, enabling better analysis and understanding of air quality dynamics in urban and rural settings.</terms:abstract>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-11-01</terms:created>
        <terms:creator rdf:resource="https://github.com/PPedreg"/>
        <terms:creator rdf:resource="https://github.com/RafaOB95"/>
        <terms:creator rdf:resource="https://github.com/aiarakoa"/>
        <terms:creator rdf:resource="https://github.com/ehrlz"/>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:title xml:lang="en">Madrid air quality ontology</terms:title>
        <vann:preferredNamespaceUri rdf:resource="https://mAIRdrid.org/ontology#"/>
        <rdfs:label xml:lang="en">AirQualityOntology</rdfs:label>
    </owl:Ontology>
    


